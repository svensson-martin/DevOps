        "from vetiver import VetiverModel, VetiverAPI\n",
        "\n",
        "b = board_folder(\"data/model\", allow_pickle_read = True)\n",
        "v = VetiverModel.from_pin(b, \"penguin_model\")\n",
        "app = VetiverAPI(v, check_prototype = True)\n",
        "app.run(port = 8080)"
